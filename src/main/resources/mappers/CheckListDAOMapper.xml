<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.toyProject.dao.CheckListDAO">

	<insert id="addCheckList">
		insert into todo_checkList(cno, tno, checkList) 
		values(todo_checkList.nextval,#{tno},#{checkList})
	</insert>


</mapper>